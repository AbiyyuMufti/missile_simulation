<launch>
  <param name="robot_description" command="$(find xacro)/xacro $(find exocet_mm40_booster_description)/urdf/exocet_mm40_booster.xacro"/>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model exocet_mm40_booster -z 3.2 -P -1.5708"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="true"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>
