<?xml version="1.0" ?>
<robot name="exocet_mm40" xmlns:xacro="http://www.ros.org/wiki/xacro" >

<xacro:property name="body_color" value="Gazebo/Silver" />

<gazebo>
  <plugin name="exocet_mm40_controller" filename="libgazebo_ros_control.so">
    <robotNamespace>exocet_mm40_controller</robotNamespace>
    <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
  </plugin>
</gazebo>

<gazebo>
  <plugin name="missile_control" filename="libmissile_control.so">
    <missile_body>missile_body</missile_body>
    <booster>Booster_1</booster>
    <booster_joint>Rigid19</booster_joint>
    <forward_dir>0 0 1</forward_dir>
  </plugin>
</gazebo>

<gazebo reference="missile_body">
  <material>${body_color}</material>
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <selfCollide>true</selfCollide>
  <gravity>true</gravity>
</gazebo>

<gazebo reference="Fin_1">
  <material>${body_color}</material>
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <selfCollide>true</selfCollide>
</gazebo>

